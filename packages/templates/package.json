{
  "name": "@julien-lin/universal-pwa-templates",
  "version": "1.3.12",
  "description": "Service worker templates for UniversalPWA",
  "keywords": [
    "pwa",
    "progressive-web-app",
    "universal-pwa",
    "service-worker",
    "workbox",
    "sw-templates",
    "pwa-templates",
    "service-worker-templates",
    "workbox-templates",
    "spa-pwa",
    "ssr-pwa",
    "static-pwa",
    "wordpress-pwa",
    "php-pwa"
  ],
  "author": "julien-lin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-lin/UniversalPWA.git",
    "directory": "packages/templates"
  },
  "homepage": "https://github.com/julien-lin/UniversalPWA#readme",
  "bugs": {
    "url": "https://github.com/julien-lin/UniversalPWA/discussions"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "README.fr.md"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "lint": "eslint src --ext .ts",
    "test": "vitest run"
  },
  "dependencies": {
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.17",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vitest": "^4.0.17"
  }
}
