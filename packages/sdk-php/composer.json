{
  "name": "julien-lin/universal-pwa-sdk-php",
  "description": "UniversalPWA PHP SDK for Laravel/Symfony integration",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "symfony/config": "^6.4 || ^7.0",
    "symfony/dependency-injection": "^6.4 || ^7.0",
    "symfony/http-kernel": "^6.4 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "UniversalPwa\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "UniversalPwa\\Laravel\\PwaServiceProvider"
      ],
      "aliases": {
        "Pwa": "UniversalPwa\\Laravel\\PwaFacade"
      }
    }
  }
}
