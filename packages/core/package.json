{
  "name": "@julien-lin/universal-pwa-core",
  "version": "1.1.2",
  "description": "Moteur de scan, génération et injection pour UniversalPWA",
  "keywords": ["pwa", "progressive-web-app", "workbox", "service-worker", "manifest"],
  "author": "julien-lin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-lin/UniversalPWA.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/julien-lin/UniversalPWA#readme",
  "bugs": {
    "url": "https://github.com/julien-lin/UniversalPWA/issues"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "lint": "eslint src --ext .ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@julien-lin/universal-pwa-templates": "workspace:^",
    "dom-serializer": "^2.0.0",
    "domhandler": "^5.0.3",
    "fs-extra": "^11.3.3",
    "glob": "^13.0.0",
    "htmlparser2": "^10.0.0",
    "pino": "^9.14.0",
    "sharp": "^0.34.5",
    "workbox-build": "^7.4.0",
    "workbox-cacheable-response": "^7.4.0",
    "workbox-expiration": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.4",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "~5.9.3",
    "vitest": "^2.1.4"
  }
}
