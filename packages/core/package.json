{
  "name": "@julien-lin/universal-pwa-core",
  "version": "1.3.11",
  "description": "Core engine for scanning, generation, and injection for UniversalPWA",
  "keywords": [
    "pwa",
    "progressive-web-app",
    "universal-pwa",
    "pwa-generator",
    "pwa-scanner",
    "pwa-injector",
    "workbox",
    "service-worker",
    "manifest",
    "web-manifest",
    "pwa-manifest",
    "auto-detect",
    "framework-detection",
    "zero-config",
    "multi-framework"
  ],
  "author": "julien-lin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-lin/UniversalPWA.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/julien-lin/UniversalPWA#readme",
  "bugs": {
    "url": "https://github.com/julien-lin/UniversalPWA/discussions"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "README.fr.md"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@julien-lin/universal-pwa-templates": "workspace:^",
    "dom-serializer": "^2.0.0",
    "domhandler": "^5.0.3",
    "fs-extra": "^11.3.3",
    "glob": "^13.0.0",
    "htmlparser2": "^10.0.0",
    "minimatch": "^9.0.3",
    "pino": "^10.2.0",
    "sharp": "^0.34.5",
    "workbox-build": "^7.4.0",
    "workbox-cacheable-response": "^7.4.0",
    "workbox-expiration": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.17",
    "pino-pretty": "^13.1.3",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vitest": "^4.0.17"
  }
}