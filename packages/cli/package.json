{
  "name": "@julien-lin/universal-pwa-cli",
  "version": "1.3.2",
  "description": "CLI to transform any web project into a PWA",
  "keywords": [
    "pwa",
    "progressive-web-app",
    "universal-pwa",
    "pwa-cli",
    "pwa-generator",
    "pwa-converter",
    "transform-to-pwa",
    "cli",
    "command-line",
    "workbox",
    "service-worker",
    "zero-config",
    "auto-detect",
    "framework-detection",
    "multi-framework",
    "wordpress-pwa",
    "react-pwa",
    "vue-pwa",
    "symfony-pwa",
    "laravel-pwa"
  ],
  "author": "julien-lin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-lin/UniversalPWA.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/julien-lin/UniversalPWA#readme",
  "bugs": {
    "url": "https://github.com/julien-lin/UniversalPWA/discussions"
  },
  "type": "module",
  "bin": {
    "universal-pwa": "dist/index.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "README.fr.md"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "lint": "eslint src --ext .ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@julien-lin/universal-pwa-core": "workspace:^",
    "chalk": "^5.6.2",
    "commander": "^12.1.0",
    "fs-extra": "^11.3.3",
    "glob": "^13.0.0",
    "inquirer": "^12.0.0",
    "pino": "^9.14.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.4",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "~5.9.3",
    "vitest": "^2.1.4"
  }
}